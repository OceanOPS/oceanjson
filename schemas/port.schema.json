{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
	  "port_country_code": {
		"type": "string",
		"minLength": 2,
		"maxLength": 2,
		"description": "Two-letter country code of the port"
	  },
	  "port_code": {
		"type": "string",
		"minLength": 3,
		"maxLength": 3,
		"description": "Three-letter code of the port"
	  },
	  "port_code_full": {
		"type": "string",
		"minLength": 5,
		"maxLength": 5,
		"description": "Full code of the port (combination of country code and port code)"
	  },
	  "port_code_name": {
		"type": "string",
		"description": "Full name of the port"
	  },
	  "port_code_name_simple": {
		"type": "string",
		"description": "Simplified name of the port"
	  }
	},
	"required": ["port_country_code", "port_code", "port_code_full", "port_code_name", "port_code_name_simple"]
  }