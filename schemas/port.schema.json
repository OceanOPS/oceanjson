{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
	  "country_code": {
		"type": "string",
		"minLength": 2,
		"maxLength": 2,
		"description": "Two-letter country code of the port"
	  },
	  "code": {
		"type": "string",
		"minLength": 3,
		"maxLength": 3,
		"description": "Three-letter code of the port"
	  },
	  "code_full": {
		"type": "string",
		"minLength": 5,
		"maxLength": 5,
		"description": "Full code of the port (combination of country code and port code)"
	  },
	  "code_name": {
		"type": "string",
		"description": "Full name of the port"
	  },
	  "code_name_simple": {
		"type": "string",
		"description": "Simplified name of the port"
	  }
	},
	"required": ["ountry_code", "code", "code_full", "code_name", "code_name_simple"]
  }