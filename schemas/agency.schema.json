{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
	  "agency_role": {
		"type": "object",
		"properties": {
		  "role_id": {
			"type": "integer"
		  },
		  "role_name": {
			"type": "string"
		  }
		},
		"required": ["role_id", "role_name"]
	  },
	  "agency_name": {
		"type": "string"
	  },
	  "agency_id": {
		"type": "integer"
	  },
	  "agency_country": {
		"$ref": "#/definitions/Country"
	  },
	  "agency_edmo_code": {
		"type": ["string", "null"]
	  },
	  "agency_grid_id": {
		"type": ["string", "null"]
	  },
	  "agency_email": {
		"type": ["string", "null"],
		"format": "email"
	  },
	  "agency_short_name": {
		"type": ["string", "null"]
	  },
	  "agency_description": {
		"type": ["string", "null"]
	  },
	  "agency_latitude": {
		"type": ["number", "null"],
		"minimum": -90,
		"maximum": 90
	  },
	  "agency_longitude": {
		"type": ["number", "null"],
		"minimum": -180,
		"maximum": 180
	  },
	  "agency_url": {
		"type": ["string", "null"],
		"format": "uri"
	  },
	  "agency_phone": {
		"type": ["string", "null"]
	  },
	  "agency_address": {
		"type": ["string", "null"]
	  },
	  "agency_fax": {
		"type": ["string", "null"]
	  },
	  "agency_is_manufacturer": {
		"type": "boolean"
	  },
	  "agency_is_purchaser": {
		"type": "boolean"
	  },
	  "agency_is_deployer": {
		"type": "boolean"
	  },
	  "agency_is_pogo_member": {
		"type": "boolean"
	  }
	},
	"required": [
	  "agency_id",
	  "agency_name"
	],
	"definitions": {
	  "country": { "$ref": "country.schema.json" }
	}
  }