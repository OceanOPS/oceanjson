{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://www.ocean-ops.org/oceanjson/schemas/Network.schema.json",
	"@context": "https://www.ocean-ops.org/oceanjson/context.jsonld",
	"@type": ["oceanmeta:Network", "schema:Organization"],
	"title": "Network",
	"description": "A group of platform missions.",
	"type": "object",
	"oceanMetaCode": "NET",
	"properties": {
	  "id": {
		"type": "string",
		"description": "Unique identifier for the network.",
		"oceanMetaCode": "NET-01",
		"oceanMetaType": "string"
	  },
	  "name": {
		"type": "string",
		"description": "The official name of the network.",
		"oceanMetaCode": "NET-02",
		"oceanMetaType": "string"
	  }
	},
	"required": ["id", "name"],
	"additionalProperties": false
}
  
